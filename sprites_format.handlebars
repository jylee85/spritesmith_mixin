@charset "utf-8";
{{#extend "scss"}}
{{#content "sprites-comment"}}{{/content}}{{#content "sprites"}}{{/content}}
{{#content "spritesheet"}}
${{spritesheet_info.strings.name}}: (
	{{spritesheet_info.strings.name}}: (width:{{spritesheet.px.width}}, height:{{spritesheet.px.height}}, url:'{{{spritesheet.escaped_image}}}'),
	{{#each sprites}}
	{{strings.name}}: (background-position-x:{{px.offset_x}}, background-position-y:{{px.offset_y}}, width:{{px.width}}, height:{{px.height}}),
	{{/each}}
);
{{/content}}
{{/extend}}