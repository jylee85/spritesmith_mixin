@charset "utf-8";
{{#extend "scss"}}
{{#content "sprites-comment"}}{{/content}}{{#content "sprites"}}{{/content}}
{{#content "spritesheet"}}
${{spritesheet_info.strings.name}}: (
	{{spritesheet_info.strings.name}}: ({{spritesheet.px.width}}, {{spritesheet.px.height}}, '{{{spritesheet.escaped_image}}}'),
	{{#each sprites}}
	{{strings.name}}: ({{px.offset_x}}, {{px.offset_y}}, {{px.width}}, {{px.height}}),
	{{/each}}
);
{{/content}}
{{/extend}}